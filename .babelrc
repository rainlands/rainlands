{
  "presets": [
    [
      "env",
      {
        "targets": {
          "electron": "1.8.4"
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-export-extensions",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    [
      "module-resolver",
      {
        "alias": {
          "@src": "./src",
          "@ui": "./src/ui",
          "@client": "./src/client",
          "@server": "./src/server",
          "@packages": "./src/packages",
          "@resources": "./src/resources"
        }
      }
    ]
  ]
}
